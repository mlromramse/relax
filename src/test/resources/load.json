{
	"virtualUsers": 2,
	"rampUp": 2,
	"loop": 1,
	"tasks": [
		{
		    "name": "Start call",
			"loop": 1,
			"url": "http://localhost:8080",
			"validate": {
				"contains": ["pom.xml"],
				"status": [200]
			}
		},
		{
		    "name": "Error page",
			"loop": 1,
			"url": "http://localhost:8080/404",
			"validate": {
				"status": [404]
			}
		},
		{
		    "name": "Server stats",
			"active": true,
			"loop": 1,
			"url": "http://localhost:8080/serverstats",
			"validate": {
				"contains": ["cpu%"],
				"status": [200]
			}
		}
	]
}