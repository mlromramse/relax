{
	"virtualUsers": 100,
	"rampUp": 10,
	"loop": -1,
	"tasks": [
		{
		    "name": "Start call",
			"active": true,
			"loop": 1,
			"url": "http://localhost:8080",
			"validate": {
				"contains": ["pom.xml", "!nottobefound"],
				"status": [200]
			},
			"delay": 2000
		},
		{
		    "name": "Error page",
			"active": false,
			"loop": 1,
			"url": "http://localhost:8080/404",
			"validate": {
				"status": [404]
			},
			"delay": 3000
		},
		{
		    "name": "Server stats",
			"active": true,
			"loop": 1,
			"url": "http://localhost:8080/serverstats",
			"validate": {
				"contains": ["^\{", "cpu%.:", "peekTime.:.*\d{1,3}"],
				"status": [200]
			},
			"delay": 10000
		}
	]
}